{
  // Use IntelliSense to learn about possible attributes.
  // Hover to view descriptions of existing attributes.
  // For more information, visit: https://go.microsoft.com/fwlink/?linkid=830387
  "version": "0.2.0",
  "configurations": [
    {
      "name": "ndmspc-cli point run",
      "type": "cppdbg",
      "request": "launch",
      "program": "${workspaceFolder}/build/src/ndmspc-cli",
      "args": ["point", "run"],
      "stopAtEntry": false,
      "cwd": "${fileDirname}",
      "environment": [
        {
          "name": "NDMSPC_POINT_CONFIG",
          "value": "${workspaceFolder}/macros/test/rsn/myAnalysis.json"
        },
        {
          "name": "NDMSPC_POINT_MACRO",
          "value": "${workspaceFolder}/macros/test/rsn/RsnPhiPointMacro.C"
        }
      ],
      "externalConsole": false,
      "MIMode": "gdb",
      "setupCommands": [
        {
          "description": "Enable pretty-printing for gdb",
          "text": "-enable-pretty-printing",
          "ignoreFailures": true
        },
        {
          "description": "Set Disassembly Flavor to Intel",
          "text": "-gdb-set disassembly-flavor intel",
          "ignoreFailures": true
        }
      ]
    },
    {
      "name": "ndmspc-cli point draw",
      "type": "cppdbg",
      "request": "launch",
      "program": "${workspaceFolder}/build/src/ndmspc-cli",
      "args": ["point", "draw"],
      "stopAtEntry": false,
      "cwd": "${fileDirname}",
      "environment": [
        {
          "name": "NDMSPC_POINT_CONFIG",
          "value": "${workspaceFolder}/macros/test/rsn/myAnalysis.json"
        }
      ],
      "externalConsole": false,
      "MIMode": "gdb",
      "setupCommands": [
        {
          "description": "Enable pretty-printing for gdb",
          "text": "-enable-pretty-printing",
          "ignoreFailures": true
        },
        {
          "description": "Set Disassembly Flavor to Intel",
          "text": "-gdb-set disassembly-flavor intel",
          "ignoreFailures": true
        }
      ]
    }
  ],
  "compounds": [
    {
      "name": "NDMSPC make install",
      "configurations": ["ndmspc-cli point run", "ndmspc-cli point draw"],
      "preLaunchTask": "make"
    }
  ]
}
