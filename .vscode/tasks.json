{
  "version": "2.0.0",
  "tasks": [
    {
      "taskName": "make",
      "command": "bash",
      "group": {
        "kind": "build",
        "isDefault": true
      },
      "dependsOn": ["make install"]
    },
    {
      "taskName": "make install",
      "command": "${workspaceRoot}/scripts/make.sh",
      "args": ["install"]
    }
  ]
}
