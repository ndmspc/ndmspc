{
  "ndmspc": {
    "environments": {
      "local": {
        "ndmspc": {
          "output": { "dir": "$HOME/.ndmspc/analyses/myAnalysis", "host": "" }
        }
      },
      "local-hist": {
        "ndmspc": {
          "process": {
            "type": "all"
          },
          "data": {
            "histogram": {
              "enabled": true
            }
          },
          "output": { "dir": "$HOME/.ndmspc/analyses/myAnalysis", "host": "" }
        }
      },
      "prod": {
        "ndmspc": {
          "output": {
            "dir": "/eos/ndmspc/scratch/temp/ndmspc/myAnalysis",
            "host": "root://eos.ndmspc.io/"
          }
        }
      },
      "prod-hist": {
        "ndmspc": {
          "data": {
            "histogram": {
              "enabled": true
            }
          },
          "output": {
            "dir": "/eos/ndmspc/scratch/temp/ndmspc/myAnalysis",
            "host": "root://eos.ndmspc.io/"
          }
        }
      }
    },
    "environment": "local",
    "cuts": [
      {
        "axis": "axis0-im",
        "bin": {
          "max": 10,
          "min": 10
        },
        "enabled": false,
        "rebin": 1
      },
      {
        "axis": "axis1-pt",
        "bin": {
          "max": 5,
          "min": 5
        },
        "enabled": true,
        "rebin": 7,
        "rebin_start": 4
      },
      {
        "axis": "axis2-ce",
        "bin": {
          "max": 1,
          "min": 1
        },
        "enabled": true,
        "rebin": 3,
        "rebin_start": 2
      },
      {
        "axis": "axis3-ns1",
        "bin": {
          "max": 1,
          "min": 1
        },
        "enabled": false,
        "rebin": 1
      },
      {
        "axis": "axis4-ns2",
        "bin": {
          "max": 1,
          "min": 1
        },
        "enabled": false,
        "rebin": 1
      },
      {
        "axis": "axis5-eta",
        "bin": {
          "max": 1,
          "min": 1
        },
        "enabled": false,
        "rebin": 1
      }
    ],
    "data": {
      "file": "root://eos.ndmspc.io//eos/ndmspc/scratch/alice/hyperloop/PWGLF-376/222580/AnalysisResults.root",
      "histogram": {
        "enabled": true,
        "file": "root://eos.ndmspc.io//eos/ndmspc/scratch/alice/rsn/hDataMap.root",
        "obj": "hDataMap",
        "base": "root://eos.ndmspc.io//eos/ndmspc/scratch/alice/rsn/bins",
        "filename": "AnalysisResults.root",
        "bins": [[7, 1, 2, 6]]
      },
      "directory": "phianalysis-t-hn-sparse_default",
      "objects": [
        "unlikepm:sigBg",
        "likepp+likemm:bgLike",
        "mixingpm+mixingmp:bgMixing",
        "unliketrue:mcTrue",
        "unlikegen:mcGen"
      ]
    },
    "job": {
      "inputs": []
    },
    "log": {
      "dir": "root://eos.ndmspc.io//eos/ndmspc/scratch/ndmspc/logs",
      "type": "error-only"
    },
    "output": {
      "dir": "$HOME/.ndmspc/analyses/myAnalysis",
      "host": "",
      "file": "content.root",
      "opt": "?remote=1",
      "delete": "onInit"
    },
    "process": {
      "type": "single",
      "split": [2, 10, 10]
    },
    "result": {
      "parameters": {
        "labels": [
          "RawBC",
          "RawFnc",
          "RawBCNorm",
          "RawFncNorm",
          "Mass",
          "Width",
          "Sigma",
          "Chi2",
          "Probability",
          "True",
          "Gen",
          "Eff"
        ],
        "draw": {
          "Mass": { "min": 1.015, "max": 1.022 }
        },
        "default": 2
      },
      "axes": [
        { "name": "bg", "labels": ["like", "mixing"], "default": 0 },
        {
          "name": "norm",
          "ranges": [
            { "name": "normOne", "range": [0.99, 1.0] },
            { "name": "normTwo", "range": [1.05, 1.06] }
          ],
          "default": 0
        },
        {
          "name": "fitFun",
          "labels": ["VoigtPol1", "VoigtPol2"],
          "default": 1
        },
        {
          "name": "fitRange",
          "ranges": [
            { "name": "frOne", "range": [0.997, 1.05] },
            { "name": "frTwo", "range": [0.995, 1.06] }
          ],
          "default": 0
        }
      ],
      "data": { "defaults": [7, 1, 2, 6] }
    },
    "file": {
      "cache": "$HOME/.ndmspc/cache"
    },
    "verbose": 0
  },
  "user": {
    "fit": {
      "default": [0.0, 1.019445, 0.00426, 0.001]
    },
    "minEntries": 1000,
    "proj": 0,
    "events": -1,
    "verbose": 0
  }
}
